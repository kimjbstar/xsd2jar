					<xs:documentation>The action performed on the EMD.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EMDInformation" type="urn:EmdInformationDetailsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Emd information of the pnr </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OfficeDutyCodeAccessType">
		<xs:sequence>
			<xs:element name="OfficeCode" type="xs:string" nillable="false">
