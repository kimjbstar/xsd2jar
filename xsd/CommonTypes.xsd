						<xs:element name="AlternatePhoneNumber" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Alternate Phone
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AlternateExtensionNumber" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Alternate Phone Extension Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FaxNumber" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Fax Number
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmailAddress" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Email Address
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SendItineraryToEmailId" type="xs:boolean"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The boolean that decides that email need to be send to the agency email when an itenary is created</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SendItineraryToPreferredEmailId" type="xs:boolean"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The boolean that decides that email need to be send to the alternate  email id of the agency when an itenary is created</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreferredEmailAddress" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>This is the alternate Email address of the agency.
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CellNumber" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Cell number of the agency
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SendItineraryToSMS" type="xs:boolean"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates to send Itinerary to source cell number as SMS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationPhone" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Destination Phone number
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SendItineraryToDestSMS" type="xs:boolean"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates to send Itinerary to destination cell number as SMS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BusinessPhoneExtension" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Business Phone Extension Number
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreferredLanguage" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Language that should be used for communications via mail</xs:documentation>
				</xs:annotation>
			</xs:element>
				<xs:element name="PhoneNumberCountryCode" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>PhoneNumberCountryCode. 
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AltPhoneNumberCountryCode" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Alternate Phone Number Country Code  of the agency
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CellNumberCountryCode" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation> Cell Number Country Code
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PNRRefundDetails">
		<xs:sequence>
			<xs:element name="PaymentStatus" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Refund or Payment Processing. Valid Values will be :REFUND or PROCESSING
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentProcessingType" type="urn:PaymentProcessingType"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This will be populated in the case of an OandD addition or Increase in party or fee deletion or ssr deletion during modify Reservation. If there is payment Processing this will be populated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RefundType" type="urn:RefundType"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Refund Details. This is required in the case of an OandD Deletion or Pnr Cancellation or decrease in party or fee and ssr deletion. This will be populated in case of a Refund.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentProcessingType">
		<xs:sequence>
			<xs:element name="FormOfPayment" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>This shows the form of Payment like AG,CC , GC,CF,CA,CK,DB,GV
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentTypeNumber" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This will be populated if the FOP has Payment Number
					Eg: for CC this will be Credit Card Number.This field should be encrypted with the supplied public key
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WriteOffCode" type="urn:WriteOffCodeType"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Write Off Code for the WO FOP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WriteOffReason" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Write Off Reason for the WO FOP
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardHolderName" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is needed in the case of Credit Card Payment. This field should be encrypted with the supplied public key
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardType" type="urn:CardType"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Needed if the form of payment is CC (Credit Card)
					Max Length: 15</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentProcessingStatus" type="xs:string"  minOccurs="0" >
				<xs:annotation>
					<xs:documentation>Valid Values are APPROVED, DECLINED or PENDING.
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExpirationMonth" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>MM This field should be encrypted with the supplied public key.
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExpirationYear" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>YY This field should be encrypted with the supplied public key
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentDescription" type="xs:string"  minOccurs="0">
			<xs:annotation>
			<xs:documentation>This field holds the payment description. The need level of this field is configurable in the system.YY This field should be encrypted with the supplied public key
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ErrorCode" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Error code for the FAILED/DECLINED transaction
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ErrorDescription" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field is not required while building the request. This field will be filled in responses for the payment transactions where the credit card payment is DECLINED/ FAILED.
					This field holds the error message or reason for the card DECLINE/FAILURE.
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConvenienceStoreCode" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The field should be set with a valid Convenience Store code. This field is mandatory only for convenience store payment transactions. Please refer appendix for valid ConvenienceStore Codes mainatained in the system. 
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConvenienceStoreName" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is an optional field.This field will be set with the convenience store name after payment transaction. In the response it will be set only if the name was specified in the request.Please refer appendix for valid ConvenienceStore Names mainatained in the system.
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConvenienceStoreType" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The field should be set with a valid Convenience Store Type. This field is mandatory only for convenience store payment transactions. Please refer appendix for valid ConvenienceStore Types mainatained in the system.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomerFirstName" type="xs:string"  minOccurs="0">
				<xs:annotation>
				 	<xs:documentation>The customer first name who had done the payment will be set in this field after convenience store payment transaction. In the request this field is not mandatory.
				 	Max Length: 51</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomerLastName" type="xs:string"  minOccurs="0">
				<xs:annotation>
				 	<xs:documentation>The customer last name who had done the payment will be set in this field after convenience store payment transaction. In the request this field is not mandatory.
				 	Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomerPhoneNumber" type="xs:string"  minOccurs="0">
				<xs:annotation>
				 	<xs:documentation>This field is mandatory for Convenience Store payment. This value can be set with the phone number provided in the preferred contact.
				 	Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecieptNumber" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field holds the convenience store settlement number. This number can be used as a reference for the convenience store payment transaction. This field will be set only after the payment transaction. In the request this field is not mandatory.
					Max Length: 32</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReceiptPrintURL" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field holds the web address of the convenience store through which the guest can make the payment. This field will be set only after Convenience store payment transaction. If SEIKO MART or SEVEN ELEVEN is selected for the Convenience Store payment,then the URL will be set for the notice of margin printing.For the other Convenience Stores the payment reference URL will be set in this field.
					Max Length: 256</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UsableCVSCompanycode" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field holds the convenience store company code where the purchaser can use the settlement vendor acceptance number. This field will be set only after Convenience store payment transaction.If more than one CVS company codes are present then it will be separated by ‘-‘ in the response.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaskedCreditcardNumber" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The credit card number in masked form for display purposes.This field will be set in the responses only.This field is not mandatory in any of the service requests.
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalCreditCardInfo" type="urn:AdditionalCreditCardInfo"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Needed if the form of payment is CC (Credit Card)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CashReceiptType" type="xs:string" minOccurs="0">
				<xs:annotation>
				        <xs:documentation>CashReceiptType of the cash receipt holder
				        Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OriginalAuthDate" type="xs:string" minOccurs="0">
				<xs:annotation>
				        <xs:documentation>Original Authorization Date in the format YYYYMMDD. This will be obtained from gateway in the case of ET authorizations.
				        Max Length: 8</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OriginalAuthCode" type="xs:string" minOccurs="0">
				<xs:annotation>
				         <xs:documentation>The authorization code obtained in the response of ET authorization.
				        Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RefundType">
		<xs:sequence>
			<xs:element name="FormOfRefund" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Form of Refund e.g.: Credit File
					Max Length: 15</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WriteOffCode" type="urn:WriteOffCodeType"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Write Off Code for the WO FOP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WriteOffReason" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Write Off Reason for the WO FOP
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CreditFileNumber" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>If the amount is refunded to CF.
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CreditFileExpiryDate" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Expriy date of the Credit file Number. YYYY-MM-DD
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RefundAmount" type="xs:double" nillable="false">
				<xs:annotation>
					<xs:documentation>Amount to be refunded
					Max Length: 19,6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FeeType" type="urn:FeeInformation"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This will be populated in case there is fee deducted for cancellation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentTypeNumber" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>This will be populated if the FOP has Payment Number
					Eg: for CC this will be Credit Card Number. Special characters including space should not be included. This field should be encrypted with the supplied public key
					Max Length: 19</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CreditCardExpiryMonth" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Credit Card expiry month in MM format
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentSubType" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Shows the SubType of FOP
					Eg: VISA for CC
					Max Length: 15</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardExpiryYear" type="xs:string"  minOccurs="0">
			<xs:annotation>
					<xs:documentation>Credit Card expiry year in YY format
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="xs:string" nillable="false">
			<xs:annotation>
					<xs:documentation>e.g.  Currency Code CAD
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaskedCreditcardNumber" type="xs:string"  minOccurs="0">
				<xs:annotation>
						<xs:documentation>The credit card number in masked form for display purposes.This field will be set in the responses only.This field is not mandatory in any of the service requests.
						Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProfileID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique ID corresponding to each customer profile.
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeeType">
		<xs:sequence>
			<xs:element name="FeeCode" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>E.g. CXL Cancellation FeeCode.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FeeAmount" type="xs:double" nillable="false">
				<xs:annotation>
					<xs:documentation>The amount paid for this fee
					Max Length: 18,5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AutomaticFeeAddedStatus" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This indicates whether system has added some cancellation fee or not.
					Valid Value: AutoMatic Fee Added.
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FeeCurrency" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Currency of Fee
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DateRangeType">
		<xs:sequence>
			<xs:element name="StartDate" type="xs:date" nillable="false">
			<xs:annotation>
					<xs:documentation>DD-MMM-YYYY</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndDate" type="xs:date" nillable="false">
			<xs:annotation>
					<xs:documentation>DD-MMM-YYYY</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PnrRepriceDetails">
		<xs:sequence>
			<xs:element name="PnrGuestDetailsType"
				type="urn:PnrGuestDetailsType" nillable="false"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contains the passenger and payment Information.
					When accessing the PnrGuestDetails from with in this object only the passenger mandatory fields and the GuestRepriceDetails will be populated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OandDDetails" type="urn:OandDDetails"
				 minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contains the OandDDetails.
					This is needed because due to an OandD Cancellation or Addition the applied tax and surcharges might change for all OandD’s. That’s why all OandD’s in the Pnr is returned. NOTE: Even though the LegDetails in Mandatory in OandDDetails only details upto segment will be populated in all Responses except availability response.
					OandD Details will not be populated from the cancel Pnr flow. This will be populated only from modify PNR flow.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AuxDetails" type="urn:AuxDetailsType"
				 minOccurs="0" maxOccurs="unbounded" >
				<xs:annotation>
					<xs:documentation>Contains Aux Details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalNonRefundableAmount"  type="xs:double" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The sum of all non-refundable amounts in the PNR.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FarePaidStatus" type="urn:FarePaidStatusType"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field indicates the fare payment status</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GuestRepriceDetails">
		<xs:sequence>
			<xs:element name="AmountToBeCollected" type="xs:double"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The amount to be paid by the passenger due to PNR Modification.
					Max Length: 18,5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmountToBeReturned" type="xs:double"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The amount to be paid back to the passenger. This applicable in the case of OandD Deletion and PNR Cancellation.
					Max Length: 18,5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Currency Code e.g. CAD
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElementType" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of Element e.g FARE/ FEE
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElementName" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of Element e.g Fare code/Fee code
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OandDChangeType">
		<xs:sequence>
			<xs:element name="ActionType" type="urn:ActionType" nillable="false">
				<xs:annotation>
					<xs:documentation>Possible Values are ADD ,MODIFY(for Cancel and Rebook) , CHANGEFLIGHT and DELETE
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OandDDetails" type="urn:OandDDetails" nillable="false">
				<xs:annotation>
					<xs:documentation>Details of the OandDs to be changed. For Modification(Cancel and rebook) we have to give the new OandD details with the Deleted OandD Id.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
		<xs:complexType name="PnrContactChangeType">
		<xs:sequence>
			<xs:element name="ActionType" type="urn:ActionType" nillable="false">
				<xs:annotation>
					<xs:documentation>Possible Values are ADD and DELETE
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PnrContact" type="urn:PnrContact" nillable="false" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Details of comment to be changed or added.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommentChangeType">
		<xs:sequence>
			<xs:element name="ActionType" type="urn:ActionType" nillable="false">
				<xs:annotation>
					<xs:documentation>Possible Values are ADD and DELETE
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentType" type="urn:CommentType"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Details of comment to be changed or added.</xs:documentation>
				</xs:annotation>
