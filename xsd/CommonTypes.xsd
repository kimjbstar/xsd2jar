					<xs:documentation>The flight segment details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PnrGuestSummaryDetails" type="urn:PnrGuestSummaryDetails" nillable="false" maxOccurs="unbounded">
