				<xs:documentation>The flight details.</xs:documentation>
			</xs:annotation>
			<xs:element name="FlightIdentifierType" type="urn:FlightIdentifierType" nillable="false">
			<xs:annotation>
					<xs:documentation>Unique identifier for the flight.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BoardPoint" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Airport code for the starting point of the segment.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OffPoint" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Airport code for the end Point of the segment.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduledDepartureTime" type="xs:dateTime" nillable="false">
				<xs:annotation>
					<xs:documentation>Scheduled departure date and time of the flight.
					YYYY-MM-DD:hh:mm:ss. LTC </xs:documentation>
				</xs:annotation>
			</xs:element>
                        <xs:element name="DepartureTimeZone" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Time zone of the place of departure in the format GMT+hh:mm
					Max Length: 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduledArrivalTime" type="xs:dateTime" nillable="false">
				<xs:annotation>
					<xs:documentation>Scheduled Arrival date and Time of the flight
					Hh:mm:ss  LTC</xs:documentation>
				</xs:annotation>
			</xs:element>
                        <xs:element name="ArrivalTimeZone" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Time zone of the place of arrival in the format GMT+hh:mm
					Max Length: 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureGate" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The departure gate.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalGate" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The arrival gate.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureStatus" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Departure Status of the flight. 
					Eg: ARRIVED, DEPARTED, ON TIME, DELAYED.
					Max Length: 10</xs:documentation>
				</xs:annotation>
				</xs:element>
			<xs:element name="ArrivalStatus" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Arrival Status of the flight. 
					Eg: ARRIVED, DEPARTED, ON TIME, DELAYED.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActualDepatureTime" type="xs:dateTime"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The actual departure date and time of the flight.
					YYYY-MM-DD:hh:mm:ss. LTC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActualArrivalTime" type="xs:dateTime"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The actual Arrival date and Time of the flight
					Hh:mm:ss  LTC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EstimatedDepartureTime" type="xs:dateTime"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The estimated departure date and time of the flight.
					YYYY-MM-DD:hh:mm:ss. LTC </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EstimatedArrivalTime" type="xs:dateTime"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The estimated Arrival date and Time of the flight
					Hh:mm:ss  LTC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="JourneyTime" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Total Time for the segment travel including the stop over time.
					hh:mm:ss
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EffectiveDate" type="urn:DateOnlyType"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The effective date of operation.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DiscontinueDate" type="urn:DateOnlyType"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The discontinue date.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>	
			<xs:element name="DaysOfOperation" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number of days of operation of the flight.
					Max Length: 7</xs:documentation>
				</xs:annotation>
			</xs:element>	
			<xs:element name="DataElements" type="urn:DataElementType"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Data elements for the flight, if DEI 201 or 210 exists.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DataElementType">
		<xs:annotation>
			<xs:documentation>This contains the DEI code and the corresponding value.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DeiCode" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>DEI name
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name ="DeiDesc" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Description for the DEI
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeiValue" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Value of the DEI.
					Max Length: 200</xs:documentation>
				</xs:annotation>
			</xs:element>			
			<xs:element name="CanDisplayDei" type="xs:boolean"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>This indicator is set true, if the DEI can be displayed.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BestFareInfo">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>The lowest fare amount for a each day.</xs:documentation>
			</xs:annotation>
			<xs:element name="FlightDate" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The departure date of the flight in its first segment..The date wil be in Format: dd-MMM-yyyy
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BestFare" type="xs:double"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The lowest fare for the corresponding flight date
					Max Length: 19,6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The lowest fare for the corresponding flight date
					The currency in which the best fare is expressed.
					Max Length: 6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DecimalPlaces" type="xs:int"  minOccurs="0"/>

		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightResults">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>The flight results.This will be the output of search availability.</xs:documentation>
			</xs:annotation>
			<xs:element name="OAndDDetails" type="urn:OandDDetails"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The OandDDetails represents each flight details along with the fare defined for it.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BestFareInfo" type="urn:BestFareInfo"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Best fare will be the lowest fare of all the flights for the specified date.
					The `BestFareDaysOut` field in the request specifies the range for which best fares are to be retrieved.</xs:documentation>
				</xs:annotation>
			</xs:element>
				<xs:element name="TripDirection" type="xs:string"  minOccurs="0">
				<xs:annotation>
				    <xs:documentation>This attribute can hold values OW or RT.
				    OW value is expected for outbound flights. RT value is expected for inbound flights.
				    Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
				<xs:element name="IsPartOfReturnTrip" type="xs:boolean"   minOccurs="0">
				<xs:annotation>
					<xs:documentation>This attribute can hold values true or false.
					True value expected when the flights of an OD are part of a round-trip, and hence RT (round trip) fares can be applied on these flights. 
					False value expected when the flights in an OD are one way flights, and hence, RT fares can not be applied to these flights</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchAvailabilityResults">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>The flight results.This will be the output of search availability.</xs:documentation>
			</xs:annotation>
			<xs:element name="OnwardFlightInfo" type="urn:FlightResults">
				<xs:annotation>
					<xs:documentation>The OandD and the best fare information of onward flights</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReturnFlightInfo" type="urn:FlightResults"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The OandD and the best fare information of return flights. This will be populated if there are flights for return segment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SearchId" type="xs:int"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The search Id specifies the search order. This is mainly used for MultiCity search. For Onward and return search the search id will be always ‘1’.
					Eg: consider a multicity search for three routes [A-B, B-C, and C-D]. In the response there will be three ‘SearchAvailabilityResults’ and for each the search id will be the order of the searched route i.e. search id will be ‘1’ for ‘SearchAvailabilityResults’ of the route A-B and ‘2’ for the B-C route and ‘3’ for the C-D route respectively.
					If any of the routes do not have flights we can identify them with the search id. If there are no flights for the route B-C then there will be only two ‘SearchAvailabilityResults’ in the response and search id of those objects will be set as ‘1’ and ‘3’.
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightSegmentSummaryDetails">
		<xs:sequence>
			<xs:element name="BoardPoint" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Board point of this segment
					Max Length: 5	</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OffPoint" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Off Point of this segment
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirlineCode" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Airline IATA Code
					Max Length: 6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightNumber" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Flight Number
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightSuffix" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Flight Suffix, the default value is *
					Max Length: 1</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightDate" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Flight date
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SegmentStatus" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The segment status
					Max Length: 25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduledDepartureDateTime" type="xs:dateTime" nillable="false">
				<xs:annotation>
					<xs:documentation>Scheduled departure date and time of the flight.
					YYYY-MM-DD:hh:mm:ss. 
					This time is in UTC. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureTimeZone" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The time zone of the place of departure in the format GMT+hh:mm
					Max Length: 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduledArrivalDateTime" type="xs:dateTime" nillable="false">
				<xs:annotation>
					<xs:documentation>Scheduled arrival date and time of the flight.
					YYYY-MM-DD:hh:mm:ss. 
					This time is in UTC.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalTimeZone" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The time zone of the place of arrival in the format GMT+hh:mm
					Max Length: 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FareLevel" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Represents the fare level involved. This will not be available for cancelled PNRs that has a refund.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalDayChange" type="xs:int" nillable="false">
				<xs:annotation>
					<xs:documentation>Difference between Arrival and Departure Date
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FareBasis" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Represents the fare basis involved.
					Max Length: 8</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightSegmentGroupID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This ID identifies the segments that are part of single connection.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PnrGuestSummaryDetails">
		<xs:sequence>
			<xs:element name="GivenName" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The first name of the guest.
