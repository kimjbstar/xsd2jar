					<xs:documentation>The pnr number which comes in the search criteria.
					Max Length: 15</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightSegmentSummaryDetails" type="urn:FlightSegmentSummaryDetails"  minOccurs="0" maxOccurs="unbounded">
