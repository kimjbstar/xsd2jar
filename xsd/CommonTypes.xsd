<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"  xmlns:urn="http://www.ibsplc.com/iRes/simpleTypes/" targetNamespace="http://www.ibsplc.com/iRes/simpleTypes/">
	<xs:include schemaLocation="SimpleTypes.xsd"/>
	<!-- <xs:include schemaLocation="RES_CommonTypes.xsd"/> -->
	<xs:annotation>
		<xs:documentation xml:lang="en">
       Search Guest Request schema for the Kisok and Web Checkin
  </xs:documentation>
	</xs:annotation>
	<xs:attributeGroup name="FlightIndicatorAttributes">
		<xs:annotation>
			<xs:documentation xml:lang="en">Provides information concerning flight operations.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="IROP_Indicator" type="xs:string">
			<xs:annotation>
				<xs:documentation> The indicator showing the irregular flight operation.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="WeatherIndicator" type="xs:string">
			<xs:annotation>
				<xs:documentation> The indicator that shows the weather condition.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:complexType name="PNRAmount">
		<xs:sequence>
			<xs:element name="AmountPaid"/>
			<xs:element name="FormOfPayment"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FareQuoteDetailsType">
		<xs:sequence>
			<xs:element name="TaxBreakDownType" type="urn:TaxDetailsPerGuest"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contains the tax break down per passenger.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SurChargeBreakDown" type="urn:SurChargeDetailsPerGuest"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contains the surcharge break down per passenger.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FareDetails">
		<xs:sequence>
			<xs:element name="BaseFare" type="xs:double">
				<xs:annotation>
					<xs:documentation>Base Fare Amount.
					Max Length: 19,6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FareLevel" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Fare Level Code
					Possible Values:
					S-Charter
					R-Regular
					A-Airmiles
					E-Companion
					D-WebOnlySale
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FareBasisCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FareBasis
					Max Length: 8</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Currency Code e.g. CAD
					Max Length: 6</xs:documentation>
				</xs:annotation>
			</xs:element>
				<xs:element name="ChildBaseFare" type="xs:double">
			<xs:annotation>
				<xs:documentation>Base fare for CHILD pax type
				Max Length: 19,6</xs:documentation>
			</xs:annotation>
		     </xs:element>
		     <xs:element name="InfantBaseFare" type="xs:double">
			<xs:annotation>
				<xs:documentation>Base fare for INFANT pax type
				Max Length: 19,6</xs:documentation>
			</xs:annotation>
		     </xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RepriceDetails">
		<xs:sequence>
			<xs:element name="CancellationFee"/>
			<xs:element name="FormOfRefund"/>
			<xs:element name="RefundAmount"/>
			<xs:element name="CreditCardNumber"/>
			<xs:element name="CreditFileNumber"/>
			<xs:element name="CreditFileExpirtyDate"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightIdentifierType">
		<xs:annotation>
			<xs:documentation>Unique Identifier for a flight.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fltNumber" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The number assigned to a particular flight in the system..eg 121
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="airlineCode" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>IATA code for the airline.
					Max Length: 6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="flightDate" type="urn:DateOnlyType" nillable="false">
				<xs:annotation>
					<xs:documentation>The departure date of the flight in its first segment. YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fltSuffix" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Suffix of the flight. This is used to differentiate between flights having same flight numbers having same FlightDate
					Max Length: 1</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierCode" type="xs:string" nillable="false">
							<xs:annotation>
								<xs:documentation>IATA CarrierCode. The CarrierCode will be the Operating Carrier Code if the Flight is a codeshare flight.
								Max Length: 5</xs:documentation>
							</xs:annotation>
						</xs:element>
                        <xs:element name="carrierFltNumber" type="xs:int"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The OperatingFlight number will be filled if the flight is a codeshare flight.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierInfo" type="xs:string"  minOccurs="0">
				<xs:annotation>
								<xs:documentation>In case of codeshare or wet lease flights, this field will have operating carrier name.
								Max Length: 50</xs:documentation>
			       </xs:annotation>
			</xs:element>

		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BookingChannelKeyType">
		<xs:sequence>
			<xs:element name="ChannelType" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identification for the channel Type making the request. It  is configurable through the application.
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Channel" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The channel defined for the channel type. This is configurable through the application.
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Locale" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">This is the locale of the channel. For example e.g: en_US. Max Length: 7</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SessionId" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">This is the session id of the user.This is used for Travel Agent authentication.
					Max Length: 128</xs:documentation>
				</xs:annotation>
			</xs:element>			
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SeatMapDetailsType">
		<xs:sequence>
			<xs:element name="SMTCode" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Seat Map Template Code. Template Code used to create the seat map
					Max Length: 25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OffPoint" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Off Point airport Code
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BoardPoint" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Board Point airport Code
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChildBoardPoint" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Same as Off Point and board Point if there is no equipment Change
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChildOffPoint" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Same as OffPoint and boardPoint if there is no equipment Change
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeckDetails" type="urn:DeckDetailsType"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of the decks in the SeatMap</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChildSegmentInfo" type="urn:SegmentInfo"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>If there is no equipment change this will be set as the current segment itself.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightIdentifier" type="urn:FlightIdentifierType" nillable="false">
				<xs:annotation>
					<xs:documentation>Unique Identifier For the Flight.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeckDetailsType">
		<xs:sequence>
			<xs:element name="DeckName" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Name given to the Deck in SeatMap
					Max Length: 15</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalSeats" type="xs:int" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Total Seats in the deck.
					Max Length: 3</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatAttributesPosition" type="urn:SeatAttributesPositionType"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">The position of the attributes Door, Overwing and Emergency Exit.This value is Conditional.This value will be populated if its defined in the template.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinDetails" type="urn:CabinDetailsType" nillable="false" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">The Cabins in the Deck. This value is Mandatory.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CabinDetailsType">
		<xs:sequence>
			<xs:element name="CabinName" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Name of Cabin
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinId" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identifier for the Cabin
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartRow" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Starting row number of the cabin
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndRow" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Ending Row of the Cabin
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalSeats" type="xs:integer" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Total Number of seats in Cabin
					Max Length: 3</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CompartmentDetails" type="urn:CompartmentDetailsType" nillable="false" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">The Compartments in the cabin .Each seat configuration-row defined in the template corresponds to a compartment.This value is Mandatory.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CompartmentDetailsType">
		<xs:sequence>
			<xs:element name="CompartmentId" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identifier for the Compartment
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartRow" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Starting Row number of the compartment
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndRow" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">End Row Number of the Compartment
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatConfiguration" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Configuration of seats in compartment eg:3-2-3
					Max Length: 30</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InternalSeatConfiguration" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Column Configuration of the Compartment
					Max Length: 30</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatDetails" type="urn:SeatDetailsType" nillable="false" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Details of the Seats in each compartment.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SeatDetailsType">
		<xs:sequence>
			<xs:element name="SeatNumber" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Seat number
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InternalRowNumber" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Internal row representation of the seat sumber. This is unique in a deck and its used to identify the order of the row numbers.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InternalColumnNumber" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Internal column representation of the seat number. This is to identify the order of the columns.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExternalRowNumber" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">External row number identifier. This is the row number defined while creating the template.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExternalColumnName" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">External Column Name
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LocationAttribute" type="xs:string"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Location attributes for the seat. Physical location in the aircraft. For example Aisle, Window
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FacilityAttribute" type="xs:string"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Facility attributes for the seat e.g.: Smoking, Wheel Chair, Leg Room and Non Reclining.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ZoneAttribute" type="xs:string"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Zone attributes for the seat. Physical zone in which the seat is defined in the aircraft.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatPriorityAttribute" type="xs:string"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Priority attributes for the seat eg:.Priority 1, priority 2
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AttachedSsr" type="xs:string"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">The SSRs that are attached with the seat, will be present in this element. To attach an SSR to a seat, the seat should have the location or facility attribute, present in seat assignment business parameter, with that SSR code.
					Max Length: 6</xs:documentation>
			</xs:annotation>
			</xs:element>
			<xs:element name="PreferredSeatSsr" type="xs:string"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Passengers with these SSRs are preferred in this seat. Max Length: 6.</xs:documentation>
			</xs:annotation>
			</xs:element>
			<xs:element name="ControlAttribute" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Status of the seat e.g.
					Available
					Reserved
					Unavailable
					Hard Blocked
					Soft Blocked
					Occupied
					Checked-in
					System Unavailable,StandbyReserved,StandbyBoarded,StandbyCleared
					Restricted
					Courtesy
					Max Length: 30
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AllocatedPassengerType" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The type of passenger allocated to the seat.This field will be populated when the seat allocated is for passenger with infant(I),Group PNR(G) and Child passenger(C)
					Max Length: 1</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AllowedFareClasses" type="xs:string"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Fare classes for the seat. If it is not present all fare classes will be allowed to seated. Eg:F,V,Y etc</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RestrictedFareClasses" type="xs:string"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">This field indicates restricted fare classes for a seat in the row. The fare classes which is configured as allowed for some particular seat row(s), will have the restriction to be seated in the seats of others rows. Such restricted fare class informations for the seat will be available in this new field.Eg: If fare classes B, L is configured for seats from 8-15, the  fare classes BandL are restricted for the seats of all rows other than  row#8-15, and will be shown in RestrictedFareClasses field of seats other than  row#8-15.Based on the pax's fare class in PNR and the values in this 'RestrictedFareClasses' field, each seat needs to be enabled or disabled during the display of seat map through WEB</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="SeatPreference" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Seat Preference</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatAssignMentFee" type="urn:SeatAssignMentFeeType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="en">The fee details of the seat.
						This is populated only when the seatmap details are returned in ShowSeatMap Service.(The boolean isSeatFeeRequired should be set as true in the Request.)</xs:documentation>
					</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SeatAttributesPositionType">
		<xs:sequence>
			<xs:element name="AttributeCode" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Code for the attribute
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AttributeName" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Name of the attribute
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartRow" type="xs:integer" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Start row number for the attribute
					Max Length: 4</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndRow" type="xs:integer" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">End Row number for the attribute
					Max Length: 4</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PositionIndicator" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Position of the attribute in the seat map (FD-Front Door, BD-BackDoor)
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LocationIndicator" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Location of the attribute in the seat map(L-Left,R-Right,C-Center )
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SeatNumberType">
		<xs:sequence>
			<xs:element name="OldSeatNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Old Seat Number of the guest.This is optional.Used only for information purpose.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NewSeatNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">New Seat Number to be assigned to the guest.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="AirportType">
		<xs:sequence>
			<xs:element name="AirportCode" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">IATA code for the airport.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirportName" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Name of the airport</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryCode" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">IATA code for the country</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryName" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Name of the country</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GuestSeatDetails">
		<xs:sequence>
			<xs:element name="SurName" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">FamilyName or LastName
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LocalSurName" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="en">Family Name or Last Name of the guest in Unicode for local language support
						Max Length: 64</xs:documentation>
