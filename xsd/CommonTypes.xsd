				</xs:annotation>
			</xs:element>
				<xs:element name="GroupName" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of Group. 
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GroupType" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation> Type of Group. 
					Max Length: 20</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
