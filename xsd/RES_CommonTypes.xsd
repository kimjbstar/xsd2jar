				<xs:annotation>
					<xs:documentation>boardPoint of the segment
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="offPoint" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>off point of the segment.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SegmentId" type="xs:long" nillable="false">
				<xs:annotation>
					<xs:documentation>The Segment Id to uniquely identify the segment.
					Max Length: 25</xs:documentation>
				</xs:annotation>
