					<xs:documentation>Can be at Creation or Usage of the Gift Voucher.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgencyCode" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Unique Identifier for an agency if the agency category is Travel Agency.
					Max Length: 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartDate" type="urn:DateOnlyType" nillable="false">
				<xs:annotation>
					<xs:documentation>Start date for a VPP.
					Max Length: 18</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndDate" type="urn:DateOnlyType"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Expiry Date for a VPP.
					Max Length: 18</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContactType" type="urn:ContactInformation" nillable="false">
				<xs:annotation>
					<xs:documentation>Specifies the Contact details of the travel agency.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VPPStatus" type="urn:VPPStatus" nillable="false">
				<xs:annotation>
					<xs:documentation>Indicates the status of the VPP. Can be Active or Inactive.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SingleGVInd" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator to specify whether commission is given for Single Gift Vouchers.
					Max Length: 1</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BulkGVInd" type="xs:string"  minOccurs="0">
