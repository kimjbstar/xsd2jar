 <!--Added for AirAvailability API-->
   <xs:complexType name="OriginDestinationInfo">
      <xs:sequence>
         <xs:element name="TripInfo" type="urn:TripInfoType"  minOccurs="0" maxOccurs="unbounded">
         <xs:annotation>
					<xs:documentation>This element will contains various flight Information that match the search provided</xs:documentation>
				</xs:annotation>
			</xs:element>
         <xs:element name="PricingInfo" type="urn:PricingInfoType"  minOccurs="0" maxOccurs="unbounded">
         <xs:annotation>
				<xs:documentation>This element will have price information for
					the flight for each passenger types in request 
				</xs:documentation>
				</xs:annotation>
			</xs:element>
         <xs:element name="BestFareInfo" type="urn:BestFareInfo" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>The Best fare will be the lowest adult fare of all the flights for the specified date.
               The `BestFareDaysOut` field in the request specifies the range for which best fares are to be retrieved.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute type="xs:string" name="TripDirection">
      <xs:annotation>
				<xs:documentation>This attribute can hold values OW or RT.
				OW value is expected for outbound flights. RT value is expected for inbound flights.
				max Length: 2</xs:documentation>
			</xs:annotation>
		</xs:attribute>
      <xs:attribute type="xs:boolean" name="IsPartOfRoundTrip">
         <xs:annotation>
				<xs:documentation> This attribute can hold values true or false.
				True value expected when the flights of an OD are part of a round-trip, and hence RT (round trip) fares can be applied on these flights. 
				False value expected when the flights in an OD are one way flights, and hence, RT fares can not be applied to these flights</xs:documentation>
			</xs:annotation>
		</xs:attribute>
      <xs:attribute type="xs:int" name="SearchIndex">
      <xs:annotation>
				<xs:documentation> For one way search, searchIndex will be 1.
				For return trip, OriginDestinationInfo for outbound will have search Index 1 and  OriginDestinationInfo for return will have search index 2 .
				For Multicity , multiple AvailabilitySearches elements will be present in the request.We are expecting the AvailabilitySearches sorted based on travel date.The OriginDestinationInfo will be provided  with respect to each AvailabilitySearches element.Search Index in OriginDestinationInfo will be in the order of AvailabilitySearches element.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute type="xs:int" name="PricingUnitID">
		 <xs:annotation>
				<xs:documentation>PricingUnitID with respect to the searched OD.
				In case of PNR creation the PricingUnitID from AirAvailability response needs to be passed to createBooking request.
				In case of PNR modification ,the PricingUnitID from EnhancedAirAvailability needs to be passed.
				Max Length: 2</xs:documentation>
			</xs:annotation>
			</xs:attribute>
   <xs:attribute type="xs:string" name="Origin">
   <xs:annotation>
				<xs:documentation>Origin airport code
				Max Length: 5</xs:documentation>
			</xs:annotation>
			</xs:attribute>
   <xs:attribute type="xs:string" name="Destination">
   <xs:annotation>
				<xs:documentation>Destination Airport code
				Max Length: 5</xs:documentation>
			</xs:annotation>
			</xs:attribute>
   </xs:complexType>
   <xs:complexType name="TripInfoType">
      <xs:sequence>
         <xs:element name="SegmentInfo" type="urn:SegmentInfoType"  minOccurs="0" maxOccurs="unbounded">
      <xs:annotation>
					<xs:documentation>This element will have flight segment information.Suppose there is a connection flight XX 100/XX 200.Then information regarding each flight segment will be given in two segmentInfo type respectively.</xs:documentation>
				</xs:annotation>
			</xs:element>
      </xs:sequence>
      <xs:attribute type="xs:long" name="TripIndex" use="optional">
        <xs:annotation>
				<xs:documentation>The value in this field will be unique for a
				flight for a particular search and will be used to cross reference to identify the price for the trip.
				 </xs:documentation>
			</xs:annotation>
		</xs:attribute>
      <xs:attribute type="xs:string" name="Route" use="optional">
        <xs:annotation>
				<xs:documentation>Identifies the flight route</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		 <xs:attribute type="xs:string" name="BaggageAllowanceRef" use="optional">
        <xs:annotation>
				<xs:documentation>Identifies the Baggage Allowance Information</xs:documentation>
			</xs:annotation>
		</xs:attribute>
   </xs:complexType>
   <xs:complexType name="SegmentInfoType">
      <xs:sequence>
         <xs:element name="FlightIdentifierInfo" type="urn:FlightIdentifierInfoType"  minOccurs="0">
          <xs:annotation>
               <xs:documentation>Identifier for the flight</xs:documentation>
            </xs:annotation>
            </xs:element>
         <xs:element name="OperatingAirlineInfo" type="urn:OperatingAirlineInfoType"  minOccurs="0">
          <xs:annotation>
               <xs:documentation>Operating carrier information of the flight is present in this element. This element will be present in response only if the Flight is marketing flight or wet lease.</xs:documentation>
            </xs:annotation>
            </xs:element>
         <xs:element name="DepartureInfo" type="urn:TravelPointInfoType"  minOccurs="0">
          <xs:annotation>
               <xs:documentation>The Information regarding departure airport  and time  will be present in this element.</xs:documentation>
            </xs:annotation>
            </xs:element>
         <xs:element name="ArrivalInfo" type="urn:TravelPointInfoType"  minOccurs="0">
          <xs:annotation>
               <xs:documentation>The Information regarding arrival airport  and time  will be present in this element.</xs:documentation>
            </xs:annotation>
            </xs:element>
         <xs:element name="AircraftInfo" type="urn:AircraftInfoType"  minOccurs="0">
          <xs:annotation>
               <xs:documentation>This element will have information regarding type and version of  aircraft used.</xs:documentation>
            </xs:annotation>
            </xs:element>
         <xs:element name="DeiInfo" type="urn:DeiInfoType"  minOccurs="0">
          <xs:annotation>
               <xs:documentation>This element will have information regarding whether there is DEI in flight and whether it can be displayed through API.</xs:documentation>
            </xs:annotation>
            </xs:element>
         <xs:element name="SegmentAvailability" type="urn:SegmentAvailabilityType"  minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
               <xs:documentation>This element will have the information regarding seats available and availability status of different fare classes for which fare are provided in response.</xs:documentation>
            </xs:annotation>
            </xs:element>
      </xs:sequence>
      <xs:attribute type="xs:long" name="SegmentIndex" use="optional">
       <xs:annotation>
				<xs:documentation>Unique id for a particular flight segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
      <xs:attribute type="xs:int" name="Stops" use="optional">
       <xs:annotation>
				<xs:documentation>No of stops for theflight segment
