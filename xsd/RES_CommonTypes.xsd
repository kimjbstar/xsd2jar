				</xs:annotation>
			</xs:element>
			<xs:element name="AncillaryDetailsType" type="urn:AncillaryDetailsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Ancillary details</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomNameValueListType">
