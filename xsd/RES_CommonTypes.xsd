					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EMDDetailsHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
