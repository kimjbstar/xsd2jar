	<xs:complexType name="NearBySearchType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>Out Bound Information.TravelInfoType is mandatory</xs:documentation>
			</xs:annotation>
			<xs:element name="Origin" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Starting point of Journey. Three letter AirportCode/CityCode should be given.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Destination" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Destination of the Journey. Three letter AirportCode/CityCode should be given.
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelDate" type="xs:date" nillable="false">
				<xs:annotation>
					<xs:documentation>Date of the intened Journey</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecificFlightDetails" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Details of specific flight to be searched.Format in which search is to be triggered is as follows :i) For Direct flight/Throught flight – XX 10O ii) For Connection Flight – XX 100/XX 200.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PositiveDaysOut" type="xs:int" default="0" nillable="false">
				<xs:annotation>
					<xs:documentation>No of days + date of travel required. This can be set as 0, if the passenger is searching for the availability for one day. The default value is 0
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreferredTimeOfDay" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Possible Values are Morning, Evening, and AnyTime.
					If this value is not specified the value "AnyTime" Configured in iFlyRes will be used.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NegativeDaysOut" type="xs:int" default="0" nillable="false">
				<xs:annotation>
					<xs:documentation>Date of travel - No of days back required. This can be set as 0, if the passenger is searching for the availability for one day. The default value is 0
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinClass" type="xs:string"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>The cabin class in which the booking is requested.
						This can be ECONOMY,
						BUSINESS, FIRST or other Cabin Classes if
						any other is defined for the airline.
						Max Length: 15
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AvailabilitySsr" type="urn:AvailabilitySsr"  minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The user can search flights with the Ssrs specified.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="canSearchNearbyOrigin" type="xs:boolean"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>If true then availability is searched for the
						other airports in the region of specified origin.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="canSearchNearbyDestination" type="xs:boolean"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>If true then availability is searched for 
						other airports in the
						region of specified destination.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BestFarePossitiveDaysOut" type="xs:int" default="0" nillable="false">
			<xs:annotation>
					<xs:documentation>Number of positive days out that system can search for Best Fare.
						The default value is 0
						Max Length: 2
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BestFareNegativeDaysOut" type="xs:int" default="0" nillable="false">
			<xs:annotation>
					<xs:documentation>Number of negative days out that system can search for Best Fare.
						The default value is 0
						Max Length: 2
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
