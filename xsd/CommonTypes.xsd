					<xs:documentation>The start date for this schedule.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduleEndDate" type="xs:dateTime" nillable="false">
				<xs:annotation>
					<xs:documentation>The end date for this schedule.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="xs:string" nillable="false">
