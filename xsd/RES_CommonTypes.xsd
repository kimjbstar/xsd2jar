					<xs:documentation>Office Code of user.
						Max Length: 6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DutyCode" type="xs:string" minOccurs="1" nillable="false" maxOccurs="unbounded">
				<xs:annotation>
