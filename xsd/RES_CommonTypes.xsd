				<xs:annotation>
					<xs:documentation>Total Fare Amount
					Max Length: 19,6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Currency Code .
					Max Length: 6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OfferOption" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date and Time of the Offer. YYYY-MM-DDThh:mm:ss   LTC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepositOption" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Time Limit details. YYYY-MM-DDThh:mm:ss   LTC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceOption" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Partial Payment details. YYYY-MM-DDThh:mm:ss   LTC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NTBAOption" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>NTBA details. YYYY-MM-DDThh:mm:ss   LTC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnreadCommentIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unread Comment Indicator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightSegmentDetails" type="urn:FlightSegmentDetailsType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Indicates the Flight Segment details.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaxCountDetails" type="urn:PaxCountDetailsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This contain the pax type and count of each pax type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepositAmount" type="urn:TotalAmountPaid" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Holds the Deposit Amount for the Group Quote.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name = "AgentID" type = "xs:string" minOccurs = "0">
				<xs:annotation>
					<xs:documentation>Agent ID of Agency  
					Max Length : 32</xs:documentation>
				</xs:annotation>
			</xs:element>

		</xs:sequence>
	</xs:complexType>

	<xs:complexType name = "CreateBookingRQ">
		<xs:complexContent>
			<xs:extension base = "urn:BasePnrRQType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="PnrType" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Type of pnr that is to be
								created, like normal, group. 
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NumberOfSeats" type="xs:int" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Number of seats
						required. This detail is also
						captured in PaxCountDetails.
						Max Length: 2
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaxCountDetails" type="urn:PaxCountDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This contain the pax type and count of each pax type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ItineraryDetails" type="urn:ItineraryDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates the itinerary details.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FareInfo" type="urn:FareInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates the pax fare details.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestDetails" type="urn:GuestRequestDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Details of the guests in the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SSRDetails" type="urn:SSRInformationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This object holds the
					details of the SSR’s that needs to be
					added in PNR.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AuxDetails" type="urn:AuxInformationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This object holds the details of the auxiliary items that needs to be opted along with PNR creation. For example the Aux detailscan be the details of car rental service.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SeatAssignmentDetails" type="urn:SeatAssignmentDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This object holds the
					details of passenger seats that needs to be
					added in PNR.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TravelDocuments" type="urn:TravelDocumentsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The travel documents associated with the guest. This can be made mandatory by a configuration in iFlyRes.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GroupPaxDetails" type="urn:GroupPaxDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Must be  filled when the PNR type is GROUP</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrContact" type="urn:PnrContactType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Contact Information in the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrComments" type="urn:CommentDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Comments entered while creating the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestLoyaltyInfo" type="urn:GuestLoyaltyInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Must be filled if the pax contains Loyalty Details</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestPaymentInfo" type="urn:GuestPaymentInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Payment Details for each passenger. A passenger can have multiple forms of payment.
					This will be null if passenger doesn’t have to make any payment for booking.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FeeInformation" type="urn:FeeRequestType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The  details of Fee that needs to be attached to the PNR</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PromoCodeDetailsType" type="urn:PromoCodeDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>PromoCode issued to the passenger.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CanContinueWithSsr" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If the Boolean is set to true, for a guest with premium seat, the fee gets automatically added, without any information given to user. If the field is set to false, the fee info is displayed to the user.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsValidateSeatSsr" type="xs:boolean" minOccurs="0"/>
					<xs:element name="PnrSessionId" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If mark seats functionality is used, then the seats for the selected O And D will marked.So if mark Seats functionality is uded pnrSessioId should be set in the response.Pnr Session Id will be returned as a respomse ot adjustFlightInventory request.
					Max Length: 10</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="isPnrUnpaid" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An indicator to set when the PNR is unpaid. This is for a specific business to integrate tour partner. This can be set true even if the passenger is fully paid for air fares.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExternalBookingNumber" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This is Tour Partner booking number corresponds to iFlyRes PNR number.
						 Max Number: 15</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="pnrOnHoldIndicator" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This indicator should be set as ‘true’, if the web is trying to create a PNR without Payment or with partial payment.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DiscountCouponDetailsType" type="urn:PaxDiscountCouponDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Array of Coupon details objects to hold the coupon details of the reservation. If the user selects a discounted fare, this field is manatory for all the passenger OandD combination. Web site need to collect the discount coupon details for all the passenger OandD combination from user if the user selects a discounted fare. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExternalRecordLocatorDetailsType" type="urn:ExternalRecordLocatorDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This field needs to be set to update the iFlyRes system with external record locater. The same has been added in retrieveBooking response to retrieve the external record locater details.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PointOfOrigin" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Point of Origin of the segment.
					Max Length: 3</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Currency" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This attribute represents the currency code of the display currency.Mandatory field
					Max Length: 6</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "CustomFieldList" type = "urn:CustomFieldListType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Contains details about custom fields</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name = "CreateBookingRS">
		<xs:complexContent>
			<xs:extension base = "urn:BasePnrRSType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="PnrNumber" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Unique Number for the reservation record.
					Max Length : 15</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CreationDateAndTime" type="xs:dateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date and Time of the
					PNR Creation in YYYY-M
					M-DDThh:mm:ss  format provided in GMT
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CreationDateTimeZone" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>TimeZone of the PNR Creation Time
					Max Length : 15</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LastModifiedDateAndTime" type="xs:dateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date and Time of the
					PNR Creation in YYYY-M
					M-DDThh:mm:ss  format provided in GMT
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LastModfiedDateTimeZone" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>TimeZone on which the last  PNR Modification happened
					Max Length: 9</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OriginalCaller" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Name of the person who requested this PNR Creation.
					Max Length : 208</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrStatus" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Status of the PNR.
					Max Length : 30</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Itinerary" type="urn:ItineraryDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates the itinerary details.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ItinPrice" type="urn:ItinPriceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates the pax fare details.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestDetails" type="urn:GuestReponseDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Information of the passengers in the itinerary.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaxBaggageAllowance" type="urn:PaxBaggageAllowanceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Baggage allowance for passenger</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SSRDetails" type="urn:SSRInformationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If there are SSRs attached to the PNR, this type will be populated.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AuxDetails" type="urn:AuxInformationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If there are Auxillary’s
				attached to the PNR,
				this will be
				populated.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SeatAssignmentDetails" type="urn:SeatAssignmentDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The seat Details of the pax in PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TravelDocuments" type="urn:TravelDocumentsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The travel documents associated with the guest.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GroupPaxDetails" type="urn:GroupPaxDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The Group details of the Group PNR </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrContact" type="urn:PnrContactType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Contact Information for this Reservation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrComments" type="urn:CommentDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If comments are added in the PNR this will have the value.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestLoyaltyInfo" type="urn:GuestLoyaltyInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Will be filled if the pax
					contains Loyalty Details
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestPaymentInfo" type="urn:GuestPaymentInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Payment Details for each passenger. A passenger can have multiple forms of payment.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FeeInformation" type="urn:FeeInformationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Fee Details of the SSR ,Itinerary</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "EMDInformation" type = "urn:EmdInformationDetailsType" minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>Emd information of the pnr </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PromoCodeDetails" type="urn:PromoCodeDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Details of the promotional code details, if any applied on the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsContinueWithSsr" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If the Boolean is set to true, for a guest with premium seat, the fee gets automatically added, without any information given to user. If the field is set to false, the fee info is displayed to the user.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsContinueWithoutSsr" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If the field is set to false, the seat will be added even if no SSR is found in the system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrSessionId" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>.If mark seats functionality is used ,then the seats for the selected O And D will marked.So if mark Seats functionality is uded pnrSessioId should be set in the response.Pnr Session Id will be returned as a respomse ot adjustFlightInventory request.
					Max Length: 10</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="isPnrUnpaid" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An indicator to set when the PNR is unpaid. This is for a specific business to integrate tour partner. This can be set true even if the passenger is fully paid for air fares.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TourCode" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Tour Code in tour operator system
						Max Length : 15
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExternalBookingNumber" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This is Tour Partner
						booking number that
						corresponds to iFlyRes
						PNR number.
						Max Number: 15
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="pnrOnHoldIndicator" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>"True" if the PNR is partially paid, or no payment has been made</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DiscountCouponDetails" type="urn:PaxDiscountCouponDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Array of Coupon details objects to hold the discount coupon details of the reservation. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExternalRecordLocatorDetails" type="urn:ExternalRecordLocatorDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This field will be set in retrieveBooking and in createBooking responses if the ExternalRecordLocatorDetails is available for the PNR. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FarePaidStatus" type="urn:FarePaidStatusDetails_Type" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This field indicates the fare payment status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AmountPaid" type="urn:AmountPaidType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If payment is made for the PNR this object will be populated.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EncodedAmountToBeShown" type="urn:PnrPaymentEncodedType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Total Amount for the PNR in Encoded Format. This field will be set based on the configuration for encoding.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalAmountDetails" type="urn:TotalAmountDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Total amount details</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalAmountToBePaid" type="urn:TotalAmountToBePaidDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Holds the total amount pending for the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalAmountPaid" type="urn:TotalAmountPaid" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Holds the total amount paid for the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PointOfOriginCurrency" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Currency code for the point of origin of the PNR
					 Max Length : 6</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PointOfOrigin" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Point Of Origin of the first departed segmant.
					 Max Length : 6</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PricingModel" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This field indicates the pricing model in which the PNR is priced initially.Possible values are IT for Itinerary Pricing and OD for OD pricing.
					Max Length : 2</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PointOfSale" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This is the two-letter country code for the Point of Sale.
					Max Length : 3</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MsgForSeatAutoFee" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This  will be populated on seat addition and gives the inforamtion about the type of seat added and the Fee amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TimeLimitDetails" type="urn:PNRTimeLimitDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If Time limit is added to the PNR then this field will be filled with the details of Time Limit applied.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ErrorType" type="urn:ErrorType" minOccurs="0"/>
					<xs:element name="PaymentAuthorisationResponse" type="urn:PaymentAuthorisationResponseType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Payment authorization response details. This will be populate only in case of two step payment process like 3D</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "CustomFieldList" type = "urn:CustomNameValueListType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Field to pass custom details as key-value pair</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RichContentInfo" type="urn:RichContentInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Holds the Rich Content Information</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Warnings" type="urn:WarningType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Used in conjunction with the Success element to define one or more business errors.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name = "ModifyBookingRQ">
		<xs:complexContent>
			<xs:extension base = "urn:BasePnrRQType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name = "isLoyaltyCancellation" type = "xs:boolean" minOccurs = "0">
						<xs:annotation>
							<xs:documentation>This is used only when isCancelPnr is true.Penality computation Engine use this field to compute the penality for cancellation.Normally it is used to apply a lower fee or no fee</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "isAirlineInitiatedChange" type = "xs:boolean" minOccurs = "0">
						<xs:annotation>
							<xs:documentation>The indicator can be set to do airline initiated cancellations without change cancellation fees. The indicator is access controlled.</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name = "canPutOnHold" type = "xs:boolean"  minOccurs = "0">
						<xs:annotation>
							<xs:documentation>This indicator should be set as 'true', if the web is trying to add an OandD or pax without Payment or with partial payment.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "PnrNumber" type = "xs:string" minOccurs = "0">
						<xs:annotation>
							<xs:documentation>Unique Number for the
						reservation record.
						Max Length : 15
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "ProfileId" type = "xs:string"  minOccurs = "0">
						<xs:annotation>
							<xs:documentation>A unique system
							generated identifer for a
							profile in the system. In
							this case, it’s the profile
							id of the logged in. This will be updated to
							the PNR only if Booking
							ContactChangeType is
							present
							Max Length : 20
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "PnrSessionId" type = "xs:string"  minOccurs = "0">
						<xs:annotation>
							<xs:documentation>.If mark seats functionality is used ,then the seats for the selected O And D will marked.So if mark Seats functionality is uded pnrSessioId should be set in the response.Pnr Session Id will be returned as a respomse ot adjustFlightInventory request.
						Max Length : 20</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "ItineraryChangeType" type = "urn:ItineraryChangeType" minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>This needs to be given if Itinerary change is needed</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "FareInfo" type = "urn:FareInfoType" minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>Used to hold fare information of newly added segment.Also used to override the fare details of existing fare components.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "BookingContactChangeType" type = "urn:BookingContactChangeType"  minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>This needs to be given if the contact information is to be changed.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "PnrCommentChangeType" type = "urn:PnrCommentChangeType"  minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>This needs to be given if Comments gets Added/Deleted</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "PnrTimeLimitChangeType" type = "urn:PnrTimeLimitChangeType"  minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>This needs to be given if Time limit needs to be Modified/deleted</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "PaxChangeType" type = "urn:PaxChangeType"  minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>This needs to fill if there is any PAX change is needed</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "TravelDocumentChangeType" type = "urn:TravelDocumentChangeType"  minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>This needs to fill if there is any Travel Document change is needed</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "SsrModifyType" type = "urn:SsrModifyType"  minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>This has to be done if Addition, Deletion or Modification of SSR is taking place</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "FeeModifyType" type = "urn:FeeModifyType"  minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>This needs to be filled if any modification has to be done to fee, like addition, deletion or modification of fee.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "NumberOfSeats" type = "xs:int" nillable = "false">
						<xs:annotation>
							<xs:documentation>This becomes mandatory if OandDs are added.
                        Max Length: 2</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "CabinChangeType" type = "urn:CabinChangeType"  minOccurs = "0" maxOccurs = "1">
						<xs:annotation>
							<xs:documentation>This should be provided when upgrade needs to be done.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "GuestPaymentInfo" type = "urn:GuestPaymentInfoType"  minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>Payment Details for each passenger. A passenger can have multiple forms of payment.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaxDiscountCouponChangeType" type="urn:PaxDiscountCouponChangeType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates discount coupon change details of the booking</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SeatAssignmentChangeType" type="urn:SeatAssignmentChangeType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This needs to be filled if any seat change is
					needed
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestLoyaltyChangeType" type="urn:GuestLoyaltyChangeType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This needs to be filled if any loyalty details change is needed
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AuxInformationChangeType" type="urn:AuxInformationChangeType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This needs to be filled if any auxillary details change is
					needed
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CanContinueWithSsr" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If the Boolean is set to true, for a guest withpremium seat, the fee
						gets automatically
						added, without any
						information given to
						user. If the field is set to
						false, the fee info is
						displayed to the user.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsValidateSeatSsr" type="xs:boolean" minOccurs="0"/>
					<xs:element name = "canOverrideMctViolation" type = "xs:boolean" minOccurs = "0">
						<xs:annotation>
							<xs:documentation>Indicator specifies if the MCT validation need to be overridden or not, if there happens to be violation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "isConfirmPriceRequired" type = "xs:boolean"  minOccurs = "0">
						<xs:annotation>
							<xs:documentation>Indicates whether confirm price need to be triggered during modify booking operation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "CHGCXLWaiverType" type = "urn:CHGCXLWaiverType" minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation> This is used to waive the chg or cxl fee that is applied in the same session.
	                    Here if the feecode is specified in the request the corresponding fee code will be waived. if feecode 
	                    is not given in the request then the whole fees applied in that session will be waived.
	                    The paid and segment id combinations are brought in the request but the same are for future usage
	                    and is not currently implemented in the system.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "CHGCXLOverrideType" type = "urn:CHGCXLOverrideType" minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation> This is used to override the chg or cxl fee that is applied in the same session.
                                    Here if the feecode is specified in the request the corresponding fee code will be override. if feecode 
                                    is not given in the request then the whole fees applied in that session will be override.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "CustomFieldList" type = "urn:CustomFieldListType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Contains details about custom fields</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name = "ModifyBookingRS">
		<xs:complexContent>
			<xs:extension base = "urn:BasePnrRSType">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="PnrNumber" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Unique Number for the reservation record.
					Max Length : 15</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CreationDateAndTime" type="xs:dateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date and Time of the
						PNR Creation in the format YYYY-M
						M-DDThh:mm:ss in GMT
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CreationDateTimeZone" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>TimeZone of the PNR
					Creation 
					Max Length : 15
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LastModifiedDateAndTime" type="xs:dateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date and Time on which
						the PNR is last
						modified shown in GMT
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LastModfiedDateTimeZone" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>TimeZone on which the last  PNR Modification happened
					Max Length: 9</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OriginalCaller" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Name of the person who requested this PNR Creation.
					Max Length : 208</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrStatus" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Status of the PNR like CANCELLED, ACTIVE.
					Max Length : 30
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaxCountType" type="urn:PaxCountDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This contain the pax type and count of each pax type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Itinerary" type="urn:ItineraryDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates the itinerary details.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ItinPrice" type="urn:ItinPriceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Indicates the passenger type level price details for the itinerary.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestDetails" type="urn:GuestReponseDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Information of the passengers in the itinerary.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SSRDetails" type="urn:SSRInformationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If there are SSRs attached to the PNR, this type will be populated.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaxBaggageAllowance" type="urn:PaxBaggageAllowanceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Baggage allowance for passenger</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AuxDetails" type="urn:AuxInformationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Aux Details can be Car or Insurance Details</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SeatAssignmentDetails" type="urn:SeatAssignmentDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The seat Details of the pax in PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TravelDocuments" type="urn:TravelDocumentsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The travel documents associated with the guest.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GroupPaxDetails" type="urn:GroupPaxDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The Group details of the Group PNR</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrContact" type="urn:PnrContactType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Contact details of PNR</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrComments" type="urn:CommentDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If comments are added in the PNR this will have the value.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestLoyaltyInfo" type="urn:GuestLoyaltyInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Loyalty details of the PNR</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GuestPaymentInfo" type="urn:GuestPaymentInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Payment Details for each passenger. A passenger can have multiple forms of payment.In case of refund use the same object, In this case the amount will be negative.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FeeInformation" type="urn:FeeInformationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Fee Details of the SSR ,Itinerary</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "EMDInformation" type = "urn:EmdInformationDetailsType" minOccurs = "0" maxOccurs = "unbounded">
						<xs:annotation>
							<xs:documentation>Emd information of the pnr </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PromoCodeDetails" type="urn:PromoCodeDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Details of the promotional code details, if any applied on the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsContinueWithSsr" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If the Boolean is set to true, for a guest with premium seat, the fee gets automatically added, without any information given to user. If the field is set to false, the fee info is displayed to the user.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsContinueWithoutSsr" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If the field is set to false, the seat will be added even if no SSR is found in the system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PnrSessionId" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Session Identifier for the PNR.
					Max Length: 10</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="isPnrUnpaid" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An indicator to set when the PNR is unpaid. This is for a specific business to integrate tour partner. This can be set true even if the passenger is fully paid for air fares.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TourCode" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This is the tour code in Tour
						Partner
						Max Length: 15
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExternalBookingNumber" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This is the External Booking
							Number or Tour operator specific booking number.
							Max Length : 15
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="pnrOnHoldIndicator" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>True" if the PNR is partially paid, or no payment has been made</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DiscountCouponDetails" type="urn:PaxDiscountCouponDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Array of Coupon details objects to hold the discount coupon details of the reservation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExternalRecordLocatorDetails" type="urn:ExternalRecordLocatorDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This field will be set in retrieveBooking and in createBooking responses if the ExternalRecordLocatorDetails is available for the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FarePaidStatus" type="urn:FarePaidStatusDetails_Type" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This field indicates the fare payment status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AmountPaid" type="urn:AmountPaidType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If payment is made for the PNR this object will be populated.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EncodedAmountToBeShown" type="urn:PnrPaymentEncodedType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Total Amount for the PNR in Encoded Format. This field will be set based on the configuration for encoding.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalAmountDetails" type="urn:TotalAmountDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Total amount details</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalAmountToBePaid" type="urn:TotalAmountToBePaidDetailsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Holds the total amount pending for the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PointOfOriginCurrency" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Currency code for the point of origin of the PNR
					 Max Length: 6</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PointOfOrigin" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Point of Origin of the segment.
					Max Length: 3</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PricingModel" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This field indicates the pricing model in which the PNR was initially priced. Possible values are IT for Itinerary pricing and OD for OD pricing.
					Max Length: 2</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="isBookingConfirmation" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicates the call is create booking or pricing </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PointOfSale" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This is the two-letter country code for the Point of Sale.
					Max Length: 3</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MsgForSeatAutoFee" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This  will be populated on seat addition and gives the inforamtion about the type of seat added and the Fee amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalNonRefundableAmount" type="urn:TotalNonRefAmount" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Total non refundable amount details</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TimeLimitDetails" type="urn:PNRTimeLimitDetailsType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>If Time limit is added to the PNR then this field will be filled with the details of Time Limit applied. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalAmountPaid" type="urn:TotalAmountPaid" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Holds the total amount paid for the PNR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ErrorType" type="urn:ErrorType" minOccurs="0"/>
					<xs:element name="PaymentAuthorisationResponse" type="urn:PaymentAuthorisationResponseType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Payment authorization response details. This will be populate only in case of two step payment process like 3D</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RichContentInfo" type="urn:RichContentInfoType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Holds the Rich Content Information</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name = "CustomFieldList" type = "urn:CustomNameValueListType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Contains details about custom fields</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Warnings" type="urn:WarningType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Used in conjunction with the Success element to define one or more business errors.</xs:documentation>
						</xs:annotation>
					</xs:element>

				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="InsuranceProductDetailsType">
		<xs:sequence>
			<xs:element name="InsuranceProviderCountry" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field will hold the value as Pax' Country of Residence
						Max Length: 10
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Language" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This attribute represents the language.
						Max
						Length: 20
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Product Id.
						Max Length: 100
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The name of the product.
						Max Length: 100
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This attribute represents the product description
						information.
						Max Length: 100
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalPremium" type="xs:double" nillable="false">
				<xs:annotation>
					<xs:documentation>The total amount to pay incl. policy costs and
						tax.
						Max Length: 19,6
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This attribute represents the currency code of
						the
						display currency.
						Max Length: 6
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PremiumCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This attribute represents the premium code
						information.
						Max Length: 15
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InsuranceInfoDetails" type="urn:InsuranceInfoDetailsType" nillable="false">
				<xs:annotation>
					<xs:documentation>Insurance info.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="InsuranceInfoDetailsType">
		<xs:sequence>
			<xs:element name="AuxFieldDetailsType" type="urn:AuxFieldDetailsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Aux details</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ElementOverrideDetailsType">
		<xs:sequence>
			<xs:element name="ElementId" type="xs:long" nillable="false">
				<xs:annotation>
					<xs:documentation>The unique identifier for the element.
							When the ElementCategory is Fee, element ID is FeeId and when ElementCategory is Fare,
							element ID is FareComponent ID. 
							Max Length: 25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElementCategory" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>Element Category : FARE or FEE
					Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestId" type="xs:long" nillable="false">
				<xs:annotation>
					<xs:documentation>The Guest id of the passenger. 
					Max Length: 25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElementType" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>This field contains the element such as Fare, Fee etc. 
							Max Length: 5</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElementSubType" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>This field contains the payment sub element such as fare basis code, SSR code, Fee Code etc.
							Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmountInPOS" type="xs:double" nillable="false">
				<xs:annotation>
					<xs:documentation>The amount paid for the particular element type in POS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="POSCurrency" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The currecny in POS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmountInPOO" type="xs:double" nillable="false">
				<xs:annotation>
					<xs:documentation>The amount paid for the particular element type in POO</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="POOCurrency" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The currecny in POS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OverrideReason" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Overeride reason
					Max Length: 50</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ChildSsrsType">
		<xs:sequence>
			<xs:element name="SSRCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Field to store SSR code
					Max Length: 7</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubSsrCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Field to store sub SSR code
					Max Length: 7</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SSRId" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This refers to the SSRid of the child SSR in a bundle SSR. 
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PNRTransactionInfo">
		<xs:sequence>
			<xs:element name="PNRAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the Action performed on the PNR in the particular transaction.
					Max Length: 7</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PNRActionUserInfo" type="urn:PNRActionUserInfo" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specifies the details of the User who has performed the action.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PNRGeneralChangeInfo" type="urn:PNRGeneralChangeInfo" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This field specifies the generic details in the PNR.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItineraryHistoryInfo" type="urn:ItineraryHistoryInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the segments in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItinPriceHistoryInfo" type="urn:ItinPriceHistoryInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the price in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestDetailsHistoryInfo" type="urn:GuestDetailsHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the passengers in the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestTicketHistoryInfo" type="urn:GuestTicketHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the tickets issued in the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SSRDetailsHistoryInfo" type="urn:SSRDetailsHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the SSRs added to the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatAssignmentHistoryDetails" type="urn:SeatAssignmentHistoryDetails" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the seat assigned to the passengers in the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PnrContactHistoryInfo" type="urn:PnrContactHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the contact in the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PnrCommentsHistoryInfo" type="urn:PnrCommentsHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the comments in the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestPaymentHistoryInfo" type="urn:GuestPaymentHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the payments in the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FeeHistoryInfo" type="urn:FeeHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the fees in the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelDocHistoryInfo" type="urn:TravelDocHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field specifies the changes that have occurred to the traveld documents in the PNR in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="AuxHistoryInfo" type="urn:AuxHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field provides the history of the Auxiliaries (Eg:INSURANCE, CAR RENTAL) added to the PNR.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrossReferenceHistoryInfo" type="urn:CrossReferenceHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field provides history of the Cross Reference details in the PNR.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EMDDetailsHistoryInfo" type="urn:EMDDetailsHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field provides history of the EMD details in the PNR.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PNRActionUserInfo">
		<xs:sequence>
			<xs:element name="UserID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the User who has performed the action.
					Max Length: 7</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Channel" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the Channel through which this action was performed.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ModifiedDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the date and time at which the modification is done.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ModifiedDateTimeZone" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>TimeZone on which the PNR Modification happened
					Max Length: 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modifiedDateInAgentTimeZone" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Modified Date in agent time zone.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PNRGeneralChangeInfo">
		<xs:sequence>
			<xs:element name = "AgencyCode" type = "xs:string" minOccurs = "0">
				<xs:annotation>
					<xs:documentation>This field denotes the agency code associated to the PNR.
					Max Length : 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name = "AgentID" type = "xs:string" minOccurs = "0">
				<xs:annotation>
					<xs:documentation>This field denotes the agenct ID associated with the agency added in the PNR.
					Max Length : 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CorporateId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the corporate code associated to the PNR.
					Max Length : 9</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumberOfSeats" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the number of inventory seats available in the PNR in the transaction.
					Max Length : 2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PnrStatus" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the status of the PNR in the transaction.
					Max Length : 30</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlatingCarrierCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the plating carrier code of the PNR.
					Max Length : 30</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PointOfPurchase" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the Point Of Purchase set in the PNR.
					Max Length : 3</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PointOfOrigin" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field denotes the Point of origin set in the PNR. 
					 Max Length : 3</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="PaymentBalanceType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field indicates the payment balance type. Possible values are POSITIVE - indicating that the PNR is in pending state, ZERO - indicating that PNR is fully paid</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ItineraryHistoryInfo">
		<xs:sequence>
			<xs:element name="FlightSegmentHistoryInfo" type="urn:FlightSegmentHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field holds the flight segment changes for the particular transaction. This field will repeat for each action.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ItinPriceHistoryInfo">
		<xs:sequence>
			<xs:element name="GuestPriceBreakDownHistoryInfo" type="urn:GuestPriceBreakDownHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This field holds the guest price breakdown for each guest. This field will repeat for each guest-modification combination. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GuestPriceBreakDownHistoryInfo">
		<xs:sequence>
			<xs:element name="PriceBreakDownHistoryInfo" type="urn:PriceBreakDownHistoryInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Thie field holds the OD level fare data for the guest referred to in the guest Id.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestId" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field holds the guest Id for whom the price is associated with.
					Max Length: 25</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PriceBreakDownHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Denotes the change on the guest price breakdown.
					Max Length: 25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PriceBreakDown" type="urn:PriceBreakDownType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Thie field holds the fare information(fare, tax, surcharge) for each fare component for the guest referred. This will be repeated for each fare component.</xs:documentation>
				</xs:annotation>
			</xs:element>

		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="GuestDetailsHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tax paid office of the PNR
					Max Length: 100</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestDetails" type="urn:GuestReponseDetailsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Details of the guests in the PNR.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GuestTicketHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The action performed on the ticket.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TicketNumber" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">The unique identifier for the ticket.
					Max Length: 13</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestId" type="xs:long" nillable="false">
				<xs:annotation>
					<xs:documentation>This field will hold the guest id against who the ticket is associated. The guest details will be provided in the ItineraryHistoryInfo. 
					Max Length: 25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SegmentId" type="xs:long" nillable="false">
				<xs:annotation>
					<xs:documentation>This field will hold the booking segment id against which the ticket is associated. The segment details will be provided in the ItineraryHistoryInfo
								Max Length: 25</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CouponNumber" type="xs:int" nillable="false">
				<xs:annotation>
					<xs:documentation xml:lang="en">Coupon Number associated for each segment in the passenger itinerary. This information will be provided against each ticket number. There can be a maximum of 4 coupons against a ticket number. If there are more than 4 coupons, the rest will be displayed against the conjunction ticket number. Coupon number can hold values 1, 2, 3 and 4.
					Max Length: 2</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SSRDetailsHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The action performed on the SSR.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SSRDetails" type="urn:SSRInformationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>SSR's associated with the PNR which are changed in this transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SeatAssignmentHistoryDetails">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The action performed on the Seat.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatAssignmentDetails" type="urn:SeatAssignmentDetailsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Seat details associated with the PNR which are changed in this transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PnrContactHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The action performed on the Contact.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PnrContact" type="urn:PnrContactType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contact details associated with the PNR which are changed in this transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PnrCommentsHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The action performed on the comments.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PnrComments" type="urn:CommentDetailsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Comments which are associated with the PNR which are changed in this transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GuestPaymentHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The action performed on the payment.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestPaymentInfo" type="urn:GuestPaymentInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Payments which are associated with the PNR which are changed in this transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeeHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The action performed on the fees.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FeeInformation" type="urn:FeeInformationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Fees which are associated with the PNR which are changed in this transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TravelDocHistoryInfo">
		<xs:sequence>
			<xs:element name="ElementAction" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The action performed on the travel documents.
					Max Length: 10</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name = "TravelDocumentsType" type = "urn:TravelDocumentsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Travel Documents which are associated with the PNR which are changed in this transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
