	<xs:complexType name="TransactionDetails">
			<xs:annotation>
				<xs:documentation>This contains the details of the transactions from the agency.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
	            <xs:element name = "PnrNumber" type = "xs:string" minOccurs="0">
					<xs:annotation>
	                        <xs:documentation>PNR Number created/modified by the agency.
	                        Max Length: 15</xs:documentation>
					</xs:annotation>
				</xs:element>
	                <xs:element name = "PnrCreationDate" type = "xs:dateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The PNR Creation Date field holds the value of  date and time on which the PNR is created, considering the time zone of the airline default office.</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "TransactionDate" type = "xs:dateTime" minOccurs="0">
						<xs:annotation>
	                        <xs:documentation>Transaction date with Time at which the creation or modification of the PNR happened.</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "PassengerName" type = "xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This contains the name of the passenger.
							Max Length: 51</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "Age" type = "xs:int" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Passengers Age, If age is not given for the PNR then no value will be displayed.
							Max Length: 3</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "DepartureDate" type = "xs:dateTime" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Flight departure date with Time.</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "FlightNumber" type = "xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Represents the flight number.
							Max Length: 5</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "Origin" type = "xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Origin,The airport code of origin will be set in this field.
							Max Length: 5</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "Destination" type = "xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Destination, The airport code of destination will be set in this field.
							Max Length: 5</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "FareBasisCode" type = "xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This field holds the code for Fare basis in which the booking was made.
							Max Length: 8</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "BillAmount" type = "xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Bill Amount is the amount paid by the passenger in that transaction. The amount paid will include the Fare + Tax+ Surcharge. There can be negative values in this field if the payment is refunded to the passenger in a particular transaction..
							Max Length: 19,6</xs:documentation>
						</xs:annotation>
					</xs:element>
	                <xs:element name = "CancellationFee" type = "xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This filed holds the value of Fee paid as cancellation Fee.
							Max Length: 19,6</xs:documentation>
						</xs:annotation>
					</xs:element>
			        <xs:element name="AgentId" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Agent Id for which the sales details needs to be retrieved.
							Max Length: 50</xs:documentation>	
               					</xs:annotation>
					</xs:element>
		</xs:sequence>
	</xs:complexType>

